{
  "name": "@cloudtec/ngx-materialize-ui-toolkit",
  "version": "0.0.9",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:core": "ng build materialize-ui-core --prod",
    "build:playground": "ng build materialize-ui-playground",
    "build": "rimraf dist && npm run build:core && npm run build:playground && npm run build:storybook",
    "test": "ng test materialize-ui-core",
    "test:silent": "ng test materialize-ui-core --karmaConfig=./libs/materialize-ui-core/karma-silent.conf.js  --codeCoverage=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build:storybook": "npm run docs:json && build-storybook -o ./dist/storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.5",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~11.0.5",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@angular/router": "~11.0.5",
    "animate.css": "4.1.1",
    "materialize-css": "1.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/core": "11.0.2",
    "@angular-devkit/build-angular": "~0.1100.5",
    "@angular-devkit/architect": "0.1100.1",
    "@angular/cli": "~11.0.5",
    "@angular/compiler-cli": "~11.0.5",
    "@babel/core": "^7.10.2",
    "@compodoc/compodoc": "^1.1.11",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-docs": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/angular": "^6.1.11",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "babel-loader": "^8.1.0",
    "jasmine-core": "~3.6.0",
    "jasmine-reporters": "2.3.2",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "2.0.1",
    "ng-packagr": "^11.0.3",
    "protractor": "~7.0.0",
    "puppeteer": "^5.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "~9.1.1",
    "typescript": "~4.0.5",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "@angular-eslint/builder": "1.0.0",
    "@angular-eslint/eslint-plugin": "1.0.0",
    "@angular-eslint/eslint-plugin-template": "1.0.0",
    "@angular-eslint/schematics": "1.0.0",
    "@angular-eslint/template-parser": "1.0.0",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0"
  }
}
